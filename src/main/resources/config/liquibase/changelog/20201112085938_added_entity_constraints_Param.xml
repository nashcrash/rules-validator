<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Param.
    -->
    <changeSet id="20201112085938-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="param_id"
                                 baseTableName="param_converter"
                                 constraintName="fk_param_converter_param_id"
                                 referencedColumnNames="id"
                                 referencedTableName="param"/>

        <addForeignKeyConstraint baseColumnNames="converter_id"
                                 baseTableName="param_converter"
                                 constraintName="fk_param_converter_converter_id"
                                 referencedColumnNames="id"
                                 referencedTableName="converter"/>
        
    </changeSet>
</databaseChangeLog>
