<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Parameter.
    -->
    <changeSet id="20201112082737-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="parameter_id"
                                 baseTableName="parameter_converter"
                                 constraintName="fk_parameter_converter_parameter_id"
                                 referencedColumnNames="id"
                                 referencedTableName="parameter"/>

        <addForeignKeyConstraint baseColumnNames="converter_id"
                                 baseTableName="parameter_converter"
                                 constraintName="fk_parameter_converter_converter_id"
                                 referencedColumnNames="id"
                                 referencedTableName="converter"/>
        
    </changeSet>
</databaseChangeLog>
